<div class="container-fluid callout">
	<div class="timer">
		<div class="row-fluid" style="margin-top:30px;">
			<div class="center-align">
				<h2>Pre-Test Result</h2>			
			</div>
		</div>
	</div>

	<div class="main-section wrapper shadow">
		<div class="row-fluid">
			<div class="score text-left">
				<p>
					<span class="<%= test_score_style(@is_pass_test) %> text-left big-font"><%= @test_result.score %></span>
					<span class="big-font">/ <%= @test_result.number_of_question%></span>
				</p>
				<%= test_result_element(@is_pass_test) %>
			</div>						
		</div>

		<div class="row-fluid">
			<div class="span2 submit-button offset9">
				<button id="back-home" type="submit" class="m-btn blue">Back to Home</button>
			</div>
		</div>
	</div>
	
</div>