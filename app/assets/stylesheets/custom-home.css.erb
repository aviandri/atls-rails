body{
	background:#fff;
	font-size: 16px;
}

.shadow{
	-webkit-box-shadow: 1px 1px 4px rgba(50, 50, 50, 0.1);
	-moz-box-shadow: 1px 1px 4px rgba(50, 50, 50, 0.1);
	box-shadow: 1px 1px 4px rgba(50, 50, 50, 0.1);
}

.wrapper{
	border: 1px solid #d9d9d9;
	padding: 10px 10px 10px 10px;
	background: #fff;
	line-height: 1.6em;	
	margin-bottom: 20px;
}

.contained{
	background: #fff;
	padding: 25px 30px 0;
	margin-bottom: 20px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	-webkit-box-shadow: rgba(0,0,0,0.1) 0 0 0 1px inset,rgba(0,0,0,0.03) 0 0 3px 0;
	-moz-box-shadow: rgba(0,0,0,0.1) 0 0 0 1px inset,rgba(0,0,0,0.03) 0 0 3px 0;
	box-shadow: rgba(0,0,0,0.1) 0 0 0 1px inset,rgba(0,0,0,0.03) 0 0 3px 0;
}

.heading{
	margin-bottom: 20px;
}

.border-bottom{
	border-bottom: 1px solid #d9d9d9;	
	margin-bottom: 20px;
	margin-left: -30px;
	margin-right: -30px;
}
.heading-button{
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	display: inline;
	float: right;
	background: #398bce;
	color: #fff;
	font-size: 18px;
	font-weight: 400;
	padding: 20px 40px;
	text-decoration: none;
}

.status-label{
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	display: inline;
	background: #398bce;
	color: #fff;
	padding: 5px 15px;
	text-decoration: none;
}

.status-label-warning{
	background-color: #f89406;

}

.status-label-important{
	background-color: #b94a48;
	padding: 5px 25px;
}

.status-label-success{
	background-color: #468847;
	padding: 5px 22px;
}

.heading-title{
	float: left;
}

.section-title{
	margin: 0px 0 20px;
	border-bottom: 1px solid #eeeeee;	
	color: #696d73;
	font-style: italic;
}

.label-title{
	font-size: 1.2em;
	font-weight: bold;
}

.section-status.label {
	padding: 5px 8px 6px;
}


.sub-label {
	font-size: 1.0em;
}

.row-fluid .sub-section{
	margin-left: 0px; 
}

#sub-section .row-fluid{
	margin-left:0;
}
#sub-section .right{
	float:right;
}

#sub-section div{
	margin-bottom: 1%;
}

.sub-section-border{
	border-right: 1px solid #eeeeee;
}

.overlay{
	 opacity : 0.4;
     background-color: 'black';
     z-index: 5000;
}

.btn{
	background-color: #f5f5f5;
}

.strong{
	font-weight: bold;
}

.table{
	padding-left: 5%;
}


.big-font{
	font-size: 3.0em;
}

.main-section{
	height: 250px;
}
.score{
	float:none;
	margin-left:auto;
	margin-right:auto;
	text-align: center
}

.center-align{
	text-align: center
}

.score p{
	margin-top: 45px;
}

.score-text{
	margin-top : 5%;
}

.submit-button{
	margin-top: 60px;
}

.navbar .nav>li>a {
  font: bold 16px/40px "Open Sans",Helvetica,Arial,Sans-serif;
  text-transform: uppercase;
  font-weight: 300;
  border-top: 11px solid transparent;
}

.callout{padding:7% 0; background:#fff;}

div#clock span { 
	font-size: 40px; font-weight: bold;
}

.text-right{
	text-align: right;
}

.valign-center{
	vertical-align: middle;
}

.centered-table{
	margin-left: auto;
	margin-right: auto;
}

.centered-table tbody tr td{
	vertical-align: middle;	
}


.statusbar {
	position: fixed;
	top: 0;
	right: 0;
	left: 0;
	z-index: 2000;
	margin-bottom: 0;
}
.statusbar .content {
	padding: 20px 0;
	font-size: 24px;
}
.success {
	background-color: #dff0d8;
	color: #468847;
}
.notice {
	background-color: #fcf8e3;
	color: #c09853
}
.error {
	background-color: #f2dede;
	color: #b94a48;
}
.info {
	background-color: #d9edf7;
	color: #3a87ad;	
}
.statusbar .close {
	font-size: 28px;
}

.magnifier-icon{
  display: inline-block;
  width: 28px;
  height: 28px;
  margin-top: 1px;
  *margin-right: .3em;
  line-height: 32px;
  vertical-align: middle;
  background-image: url('<%= asset_path 'magnifier-icon.png' %>');
  background-repeat: no-repeat;
}

.magnifier-icon:hover{
	background-image: url('<%= asset_path 'magnifier-icon-selected.png' %>');
}

.data-item{
	padding: 20px 0;
	border-top: 1px solid #e9e9e9;
}

.section{
	margin-bottom: 30px;
}

.section-heading{
	font-weight: 700;
	font-size: 20px;
}


.body-button{
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
	display: inline;
	float: right;
	background: #398bce;
	color: #fff;
	font-size: 16px;
	font-weight: 400;
	padding: 10px 10px;
	text-decoration: none;
}

.subnote{
  display: block;
  font-size: 14px;
  color: red;
  font-weight: normal;
}

.control-group{
	overflow: hidden;
	font-size: 16px;

}
.control-group label{
	margin-top: 5px;
}

.control{
	float: right;
	width: 70%;
}

.control input{
	width: 90%;
	padding: 10px 10px;

}

label, input{
	font-size: 16px;
}

label{
	font-weight: bold;
}

.action-button{
	margin-top: 20px;
	margin-bottom: 20px;
}

.radio{
	margin-bottom: 10px;
}

.payment-method-detail{
	font-size: 16px;
}

.payment-method-detail div{
	margin-top: 20px;
}

.action-button .m-btn{
	margin-left: 0px;
	margin-right: 20px;
}

.content{
  min-height: 100%;
  position: relative;
  padding-top: 20px; 
}

@media (max-width: 480px) {
	.body-button{
		float:left;
	}

	.content{
		padding-top: 0px;
	}

	body{
		font-size: 14px;
	}

	.heading-button{
		padding: 20px 10px;
	}
	
}



